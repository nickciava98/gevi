<openerp>
  <data>
  
    <!-- actions opening views on TipoBilancia -->
    <record model="ir.actions.act_window" id="gevi_bilance.action_window_configurazione_TipoBilancia">
      <field name="name">Gevi Bilance Tipo Bilancia Window</field>
      <field name="res_model">gevi_bilance.tipobilanciatable</field>
      <field name="view_mode">tree,form</field>
    </record>
	
	<!-- Tipo Bilancia Tree -->
    <record model="ir.ui.view" id="gevi_bilance.TipoBilancia_view_tree">
      <field name="name">Tipo Bilancia Tree</field>
      <field name="model">gevi_bilance.tipobilanciatable</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
		  <!--field name="tipoBilanciaAttr"/-->
        </tree>
      </field>
    </record>
	
	<!-- Tipo Bilancia Form -->
    <record model="ir.ui.view" id="gevi_bilance.TipoBilancia_view_form">
      <field name="name">Tipo Bilancia Form</field>
      <field name="model">gevi_bilance.tipobilanciatable</field>
      <field name="arch" type="xml">
        <form string="Tipo Bilancia Form">
		  <group>
            <field name="name"/>		  
			<!--field name="tipoBilanciaAttr"/-->
		  </group>
        </form>
      </field>
    </record>
	
  </data>
</openerp>