<?xml version="1.0"?>
<odoo>
	<data noupdate="1">

		<record model="ir.sequence" id="seq_gevi_zbilance_bilancia">
			<field name="name">GEVI Codice Bilancia</field>
			<field name="code">gevi_zbilance.bilancia</field>
			<field name="prefix">BIL</field>
			<field name="padding">7</field>
		</record>

		<record model="ir.sequence" id="seq_gevi_zbilance_bilancia">
			<field name="name">GEVI Codice Verifica Bilancia</field>
			<field name="code">gevi_zbilance.verifica</field>
            <field name="prefix">ProgBil/</field>
            <field name="padding">6</field>
		</record>

		<record id="seq_gevi_zbilance_verbale" model="ir.sequence">
            <field name="name">GEVI Codice Verbale Bilance</field>
            <field name="code">gevi_zbilance.verbale</field>
            <field name="prefix">BIL %(year)s/</field>
        </record>
	</data>
</odoo>
